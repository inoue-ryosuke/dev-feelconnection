{
  "addons": [
    "herokuconnect",
    "heroku-postgresql",
    "heroku-redis"
  ],
  "buildpacks": [
    {
      "url": "heroku/php"
    }
  ],
  "env": {
    "APP_KEY": {
      "required": true
    },
    "DB_CONNECTION": {
      "required": true
    },
    "DB_DATABASE": {
      "required": true
    },
    "DB_HOST": {
      "required": true
    },
    "DB_PASSWORD": {
      "required": true
    },
    "DB_USERNAME": {
      "required": true
    },
    "REDIS_URL": {
      "required": true
    }
  },
  "formation": {
    "web": {
      "quantity": 1
    }
  },
  "name": "dev-feelconnection",
  "scripts": {
  },
  "stack": "heroku-18"
}
